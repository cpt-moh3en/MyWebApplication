@{
    ViewData["Title"] = "UserInfo2";
}


<div class="page-section">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-md-9 col-lg-6 py-3">
                <div class="card-page">
                    <h5 class="fg-primary">Partners</h5>
                    <hr>
                    @if (ViewBag.user != null)
                    {
                        @foreach (var user in ViewBag.user)
                        {
                            <div class="bg-danger p-5 text-light rounded my-5">
                              <p> Id : @user.Id </p>
                              <p> Name : @user.Name </p>
                              <p> Family : @user.Family </p>
                              <p> PhoneNumber : @user.PhoneNumber </p>
                              <p> Age : @user.Age </p>
                              <p> Address : @user.Address </p>
                              <p> DateTime : @user.DateTime </p>
                            </div>
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</div>